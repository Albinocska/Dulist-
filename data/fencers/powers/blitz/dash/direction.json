{
    "name": "Directional dash",
    "description": "Dash based on the movement key pressed.",
    "hidden": true,
    "type": "origins:multiple",
    "direction":{
        "type":"origins:resource",
        "min": 0,
        "max":4,
        "hud_render": {
            "should_render": false
        },
        "start_value": 0
    },

    "forward": {
        "type": "origins:active_self",
        "entity_action": {
            "type": "origins:change_resource",
            "resource": "*:*_direction",
            "change": 1,
            "operation": "set"
        },
        "cooldown": 1,
        "hud_render": {
            "should_render": false
        },
        "key": {
            "key": "key.forward",
            "continuous": true
        }
    },
    "back": {
        "type": "origins:active_self",
        "entity_action": {
            "type": "origins:change_resource",
            "resource": "*:*_direction",
            "change": 2,
            "operation": "set"
        },
        "cooldown": 1,
        "hud_render": {
            "should_render": false
        },
        "key": {
            "key": "key.back",
            "continuous": true
        }
    },
    "left": {
        "type": "origins:active_self",
        "entity_action": {
            "type": "origins:change_resource",
            "resource": "*:*_direction",
            "change": 3,
            "operation": "set"
        },
        "cooldown": 1,
        "hud_render": {
            "should_render": false
        },
        "key": {
            "key": "key.left",
            "continuous": true
        }
    },
    "right": {
        "type": "origins:active_self",
        "entity_action": {
            "type": "origins:change_resource",
            "resource": "*:*_direction",
            "change": 4,
            "operation": "set"
        },
        "cooldown": 1,
        "hud_render": {
            "should_render": false
        },
        "key": {
            "key": "key.right",
            "continuous": true
        }
    }
}